# --- Configuration de la base de données applicative ---
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=supervia
DATABASE_URL="postgresql://postgres:your_postgres_password@postgres:5432/supervia?schema=public"

# --- Configuration de la base de données Zabbix ---
ZABBIX_DB_USER=zabbix
ZABBIX_DB_PASSWORD=your_zabbix_db_password
ZABBIX_DB_NAME=zabbix

# --- Configuration de l'API Zabbix ---
ZABBIX_URL=http://zabbix-web:8080/api_jsonrpc.php
ZABBIX_USER=Admin
ZABBIX_PASSWORD=your_zabbix_password

# --- Secrets et URLs internes ---
JWT_SECRET=your_jwt_secret_change_in_production
INTERNAL_API_KEY=your_internal_api_key_change_in_production
DB_SERVICE_URL=http://db-service:3000
NOTIFICATION_SERVICE_URL=http://notification-service:3000
AI_SERVICE_URL=http://ai-service:3000

# --- Configuration SMTP (Mailtrap pour le développement) ---
SMTP_HOST=sandbox.smtp.mailtrap.io
SMTP_PORT=2525
SMTP_SECURE=false
SMTP_USER=your_mailtrap_user
SMTP_PASS=your_mailtrap_password
SMTP_FROM_NAME="SupervIA Alerts"
SMTP_FROM_EMAIL=alerts@notifications.supervia.local
ALERT_BATCH_DURATION="30000"
FRONTEND_URL=http://localhost:3000
NEXT_PUBLIC_AI_API_URL=http://localhost:3005/api/ai
NEXT_PUBLIC_NOTIFICATION_API_URL=http://localhost:3004
NOTIF_DEFAULT_TO=ops@supervia.local

# --- Session et OAuth ---
SESSION_SECRET=your_session_secret_change_in_production
AUTH_SERVICE_URL=http://localhost:3002
AI_SERVICE_PUBLIC_URL=http://localhost:3005

# --- Identifiants Google OAuth ---
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# --- Identifiants GitHub OAuth ---
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret 

# --- IA Provider ---
OPENAI_API_KEY=your_openai_api_key